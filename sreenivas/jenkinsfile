pipeline{
    agent(any)

    environment variable{
        dokcer-credentials:${Sreenivas}
    }

    Stages{
        stage(Gitclone){
            step(
                sh "git clone"
            )
        }
        stage(Dockerbuild){
            step(
                sh "docker build -t Sreenivas:$BUILDID"
            )
        }
    }
}